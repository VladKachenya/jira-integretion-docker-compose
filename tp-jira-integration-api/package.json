{
  "name": "tp-jira-integration-api",
  "version": "1.0.0",
  "description": "Jira projects viewer api",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --coverage --verbose",
    "dev": "tsc-watch --onSuccess \"node ./dist/index.js\"",
    "build": "tsc",
    "start": "node dist/index.js",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VladKachenya/tp-jira-integration-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "bugs": {
    "url": "https://github.com/VladKachenya/tp-jira-integration-api/issues"
  },
  "homepage": "https://github.com/VladKachenya/tp-jira-integration-api#readme",
  "dependencies": {
    "@types/compression": "^1.0.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/node": "^13.1.2",
    "@types/request-promise": "^4.1.45",
    "@types/swagger-ui-express": "^4.1.1",
    "async-middleware": "^1.2.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "path": "^0.12.7",
    "pg": "^7.17.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.5",
    "swagger-ui-express": "^4.1.2",
    "tsc-watch": "^4.0.0",
    "tsconfig-paths": "^3.9.0",
    "typeorm": "^0.2.22",
    "typescript": "^3.7.4"
  },
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "jest": "^24.9.0",
    "ts-jest": "^24.3.0"
  }
}
